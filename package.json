{
  "name": "goteo",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "preview:ci": "NODE_ENV=test NODE_TLS_REJECT_UNAUTHORIZED=0 SKIP_PAYMENT_GATEWAYS=true PAYMENT_GATEWAYS_ENABLED=false wrangler dev --env preview --port 4321 --ip 0.0.0.0",
    "astro": "astro",
    "format": "eslint --fix . && prettier --write .",
    "format:eslint": "eslint --fix . ",
    "format:prettier": "prettier --write .",
    "openapi": "openapi-ts",
    "check:format:prettier": "prettier --check . --plugin prettier-plugin-astro --plugin prettier-plugin-svelte",
    "check": "pnpm check:format:prettier",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "cypress:verify": "cypress verify",
    "test:e2e": "start-server-and-test dev http://localhost:4321 cypress:run",
    "test:e2e:ci": "NODE_TLS_REJECT_UNAUTHORIZED=0 start-server-and-test preview:ci http://localhost:4321 cypress:run",
    "dev:worker": "npx astro build && npx wrangler dev --port 4321 --ip 0.0.0.0"
  },
  "dependencies": {
    "@astrojs/cloudflare": "^12.6.0",
    "@astrojs/node": "^9.2.2",
    "@astrojs/svelte": "^7.1.0",
    "@fontsource/karla": "^5.2.5",
    "@hey-api/client-fetch": "^0.11.0",
    "@tailwindcss/vite": "^4.1.8",
    "@types/iso-3166-2": "^1.0.4",
    "astro": "^5.8.1",
    "bits-ui": "^2.4.1",
    "chart.js": "^4.4.9",
    "dotenv": "^16.5.0",
    "flickity": "^3.0.0",
    "iso-3166-2": "^1.0.0",
    "marked": "^15.0.12",
    "media-icons": "^1.1.5",
    "svelte": "^5.33.13",
    "tailwindcss": "^4.1.8",
    "typescript": "^5.8.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.28.0",
    "@hey-api/openapi-ts": "^0.69.2",
    "@types/node": "^24.0.4",
    "@typescript-eslint/eslint-plugin": "^8.33.1",
    "@typescript-eslint/parser": "^8.33.1",
    "cypress": "^14.4.0",
    "eslint": "^9.28.0",
    "eslint-plugin-cypress": "^5.0.1",
    "eslint-plugin-import": "^2.31.0",
    "flowbite": "^3.1.2",
    "flowbite-svelte": "1.5.2",
    "flowbite-svelte-icons": "^2.2.0",
    "globals": "^16.2.0",
    "prettier": "3.5.3",
    "prettier-plugin-astro": "^0.14.1",
    "prettier-plugin-svelte": "^3.4.0",
    "prettier-plugin-tailwindcss": "^0.6.12",
    "start-server-and-test": "^2.0.12",
    "typescript-eslint": "^8.33.1",
    "wait-on": "^9.0.0",
    "wrangler": "^4.37.1"
  }
}
