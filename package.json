{
  "name": "goteo",
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "astro": "astro",
    "build": "astro build",
    "check": "pnpm check:format:prettier",
    "check:format:prettier": "prettier --check . --plugin prettier-plugin-astro --plugin prettier-plugin-svelte",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "cypress:verify": "cypress verify",
    "dev": "astro dev",
    "dev:worker": "npx astro build && npx wrangler dev --port 4321 --ip 0.0.0.0",
    "format": "eslint --fix . && prettier --write .",
    "format:eslint": "eslint --fix . ",
    "format:prettier": "prettier --write .",
    "openapi": "openapi-ts",
    "preview": "astro preview",
    "preview:ci": "NODE_ENV=test NODE_TLS_REJECT_UNAUTHORIZED=0 SKIP_PAYMENT_GATEWAYS=true PAYMENT_GATEWAYS_ENABLED=false wrangler dev --env preview --port 4321 --ip 0.0.0.0",
    "test:e2e": "start-server-and-test dev http://localhost:4321 cypress:run",
    "test:e2e:ci": "NODE_TLS_REJECT_UNAUTHORIZED=0 start-server-and-test preview:ci http://localhost:4321 cypress:run"
  },
  "dependencies": {
    "@astrojs/check": "^0.9.6",
    "@astrojs/cloudflare": "^12.6.12",
    "@astrojs/node": "^9.5.1",
    "@astrojs/svelte": "^7.2.2",
    "@fontsource/karla": "^5.2.8",
    "@hey-api/client-fetch": "^0.11.0",
    "@tailwindcss/vite": "^4.1.17",
    "@tiptap/core": "^3.10.1",
    "@tiptap/extension-font-family": "^3.10.1",
    "@tiptap/extension-placeholder": "^3.10.1",
    "@tiptap/extension-text-align": "^3.10.1",
    "@tiptap/extension-text-style": "^3.10.1",
    "@tiptap/pm": "^3.10.1",
    "@tiptap/starter-kit": "^3.10.1",
    "@types/iso-3166-2": "^1.0.4",
    "astro": "^5.16.4",
    "bits-ui": "^2.14.4",
    "chart.js": "^4.5.1",
    "dotenv": "^16.6.1",
    "flickity": "^3.0.0",
    "iso-3166-2": "^1.0.0",
    "marked": "^15.0.12",
    "media-icons": "^1.1.5",
    "svelte": "^5.45.8",
    "tailwind-merge": "^3.4.0",
    "tailwindcss": "^4.1.17",
    "typescript": "^5.9.3",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@hey-api/openapi-ts": "^0.69.2",
    "@types/node": "^24.10.2",
    "@typescript-eslint/eslint-plugin": "^8.49.0",
    "@typescript-eslint/parser": "^8.49.0",
    "cypress": "^15.8.2",
    "eslint": "^9.39.1",
    "eslint-plugin-cypress": "^5.2.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-unused-imports": "^4.3.0",
    "flowbite": "^3.1.2",
    "flowbite-svelte": "1.5.2",
    "flowbite-svelte-icons": "^2.3.0",
    "globals": "^16.5.0",
    "prettier": "3.5.3",
    "prettier-plugin-astro": "^0.14.1",
    "prettier-plugin-svelte": "^3.4.0",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "start-server-and-test": "^2.1.3",
    "typescript-eslint": "^8.49.0",
    "wait-on": "^9.0.3",
    "wrangler": "^4.53.0"
  }
}
